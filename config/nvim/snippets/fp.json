{
  "Gen Function $ (wrapped)": {
    "prefix": "zgen$",
    "body": ["Effect.gen(function* ($) {\n\t$0\n})"],
    "description": "Generator function with $ input"
  },
  "Gen Yield $": {
    "prefix": "yield$",
    "body": ["yield* $($0)"],
    "description": "Yield generator calling $()"
  },
  "Gen Yield $ (const)": {
    "prefix": "cyield$",
    "body": ["const $1 = yield* $($0)"],
    "description": "Yield generator calling $()"
  },
  "Import pipe": {
    "prefix": "ipipe",
    "body": ["import { pipe } from '@effect/data/Function';"]
  },
  "Import Effect as Z": {
    "prefix": "iz",
    "body": ["import * as Effect from '@effect/io/Effect';"]
  },
  "Import Layer as ZL": {
    "prefix": "izl",
    "body": ["import * as Layer from '@effect/io/Layer';"]
  },
  "Import Option as O": {
    "prefix": "ioption",
    "body": ["import * as Option from '@effect/data/Option';"]
  },
  "Import Either as E": {
    "prefix": "ieither",
    "body": ["import * as Either from '@effect/data/Either';"]
  },
  "Import fp-ts module": {
    "prefix": "idata",
    "body": ["import * as $0 from '@effect/data/$0';"]
  },
